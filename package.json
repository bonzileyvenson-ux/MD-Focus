{
  "name": "sass",
  "version": "1.0.0",
  "description": "modulo de sass",
  "main": "index.js",
  "scripts": {
    "watch:sass": "npx sass sass/main.scss css/style.css -w --load-path node_modules",
    "compile:sass": "npx sass sass/main.scss css/style.comp.css",
    "prefix:css": "npx  postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "npx sass css/style.prefix.css css/style.css --style compressed --load-path node_modules",
    "build:css": "npx run-s compile:sass prefix:css compress:css"
  },
  "author": "Bonyve",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.93.2"
  },
  "dependencies": {
    "notie": "^4.3.1"
  }
}
